@namespace(uri="http://www.intec.edu.do/abdiel", prefix="abdiel")
package abdiel;

class Library {
	val PartSpecification[*] specs;
}

class PartSpecification {
	attr String name;
	val Property[*] properties;
	val PinSpecification[*] pins;
}

class Property {
	attr String name;
	attr String value;
}

enum Polarity {
	NEUTRAL = 1;
	POSITIVE = 2;
	NEGATIVE = 3;
}

class PinSpecification {
	attr String name;
	attr Polarity polarity;
}